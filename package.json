{
  "name": "express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/nodemon ./app/launcher.js --exec babel-node",
    "development": "npm run dev",
    "build": "./node_modules/.bin/babel app -d dist",
    "serve": "./node_modules/.bin/cross-env NODE_ENV=production node ./dist/launcher.js",
    "prod": "npm run build && npm run serve",
    "production": "npm run prod",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "basic-auth": "^1.0.4",
    "bluebird": "^3.4.6",
    "body-parser": "~1.15.1",
    "compression": "^1.6.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "^4.14.0",
    "express-redirect": "^1.2.2",
    "express-urlrewrite": "^1.2.0",
    "file-stream-rotator": "^0.0.7",
    "forcedomain": "^0.8.0",
    "jade": "~1.11.0",
    "lodash": "^4.16.6",
    "morgan": "~1.7.0",
    "response-time": "^2.3.1",
    "vhost": "^3.0.2"
  },
  "main": "app/index.js",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-closure-elimination": "^1.1.11",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.0",
    "babel-preset-node6": "^11.0.0",
    "cross-env": "^3.1.3",
    "node-sass-middleware": "^0.9.8",
    "nodemon": "^1.11.0"
  }
}
