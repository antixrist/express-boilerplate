{
  "name": "express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/nodemon ./server/index.js --exec babel-node",
    "build": "./node_modules/.bin/babel app -d dist/app --source-maps inline && ./node_modules/.bin/babel server -d dist/server --source-maps inline",
    "serve": "node dist/server/index.js",
    "test": "mocha --compilers js:babel-register"
  },
  "dependencies": {
    "basic-auth": "^1.0.4",
    "bluebird": "^3.4.6",
    "body-parser": "~1.15.1",
    "compression": "^1.6.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-redirect": "^1.2.2",
    "express-urlrewrite": "^1.2.0",
    "file-stream-rotator": "^0.0.7",
    "forcedomain": "^0.8.0",
    "jade": "~1.11.0",
    "lodash": "^4.16.5",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.8.0",
    "response-time": "^2.3.1",
    "serve-favicon": "~2.3.0",
    "vhost": "^3.0.2"
  },
  "main": "app/index.js",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.0",
    "babel-preset-node6": "^11.0.0",
    "nodemon": "^1.11.0"
  }
}
