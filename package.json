{
  "name": "express-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=app:* nodemon src/server.js --exec babel-node",
    "development": "npm run dev",
    "build": "rm -rf ./dist && mkdir dist && babel src --out-dir dist --copy-files --source-maps",
    "serve": "cross-env NODE_ENV=production node dist/server.js",
    "prod": "npm run build && npm run serve",
    "production": "npm run prod",
    "test": "mocha --compilers js:babel-register",
    "postinstall": "bower install"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-closure-elimination": "^1.1.11",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.0",
    "babel-preset-node6": "^11.0.0",
    "basic-auth": "^1.0.4",
    "bluebird": "^3.4.6",
    "body-parser": "~1.15.1",
    "compression": "^1.6.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "^3.1.3",
    "csurf": "^1.9.0",
    "death": "^1.0.0",
    "debug": "~2.2.0",
    "env-cmd": "^2.2.0",
    "express": "^4.14.0",
    "express-mysql-session": "^1.2.0",
    "express-redirect": "^1.2.2",
    "express-session": "^1.14.2",
    "express-urlrewrite": "^1.2.0",
    "file-stream-rotator": "^0.0.7",
    "forcedomain": "^0.8.0",
    "hard-rejection": "^0.1.0",
    "helmet": "^3.1.0",
    "http-errors": "^1.5.0",
    "jade": "~1.11.0",
    "lodash": "^4.16.6",
    "loud-rejection": "^1.6.0",
    "lusca": "^1.4.1",
    "morgan": "~1.7.0",
    "nodemon": "^1.11.0",
    "object-inspect": "^1.2.1",
    "path-exists": "^3.0.0",
    "portastic": "^1.0.1",
    "response-time": "^2.3.1",
    "source-map-support": "^0.4.6",
    "vhost": "^3.0.2",
    "yargs": "^6.4.0"
  },
  "main": "dist/server.js",
  "license": "MIT",
  "devDependencies": {
    "pm2": "^2.1.5"
  }
}
